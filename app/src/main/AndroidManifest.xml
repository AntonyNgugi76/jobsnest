<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jobsnest" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FiNet" >
        <activity
            android:name="com.example.jobsnest.Activities.NetIncome"
            android:exported="true" />
        <activity
            android:name="com.example.jobsnest.Activities.Register"
            android:exported="true"
            android:theme="@style/Theme.FiNet.NoActionBar"/>
        <activity
            android:name="com.example.jobsnest.Activities.LoginActivity"
            android:theme="@style/Theme.FiNet.NoActionBar"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.jobsnest.Activities.MainActivity"
            android:exported="true" >
        </activity>

        <activity
            android:name="com.example.jobsnest.Activities.AddWorkActivity"
            android:theme="@style/Theme.FiNet.NoActionBar"
            android:exported="true" >
        </activity>
    </application>

</manifest>